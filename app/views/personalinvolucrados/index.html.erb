<h1>Listing personalinvolucrados</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @personalinvolucrados.each do |personalinvolucrado| %>
      <tr>
        <td><%= personalinvolucrado.nombre %></td>
        <td><%= link_to 'Show', personalinvolucrado %></td>
        <td><%= link_to 'Edit', edit_personalinvolucrado_path(personalinvolucrado) %></td>
        <td><%= link_to 'Destroy', personalinvolucrado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Personalinvolucrado', new_personalinvolucrado_path %>
